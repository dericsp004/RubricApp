<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.plummer.deric.rubricapp.Grade_Activity">
    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:background="@android:drawable/ic_input_add"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="expand" />

    <Button
        android:id="@+id/button6"
        android:layout_width="72dp"
        android:layout_height="44dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="16dp"
        android:background="@android:drawable/presence_busy"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="collapse" />

    <ExpandableListView
        android:id="@+id/GradeList"
        android:layout_height="358dp"
        android:layout_width="277dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="24dp"
        android:layout_marginBottom="8dp"
        tools:layout_editor_absoluteY="161dp"
        tools:layout_editor_absoluteX="108dp" />

    <TextView
        android:id="@+id/textViewStudentName"
        android:layout_width="175dp"
        android:layout_height="30dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/textViewAssignmentName"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteY="15dp" />

    <TextView
        android:id="@+id/textViewAssignmentName"
        android:layout_width="165dp"
        android:layout_height="30dp"
        android:layout_marginRight="16dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textViewStudentName"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteY="16dp"
        tools:layout_editor_absoluteX="203dp" />

    <TextView

        android:id="@+id/textView3"
        android:layout_width="220dp"
        android:layout_height="46dp"
        android:id="@+id/textViewComments"
        android:layout_width="192dp"
        android:layout_height="37dp"
        android:text="Comments"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteY="426dp"
        tools:layout_editor_absoluteX="63dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="93dp"
        android:layout_height="38dp"
        android:layout_marginRight="147dp"
        android:text="Submit"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteY="469dp"
        tools:text="Submit"
        android:layout_marginLeft="147dp"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:layout_editor_absoluteX="144dp"
        app:layout_constraintHorizontal_bias="0.0" />

    <ExpandableListView
        android:id="@+id/GradeList"
        android:layout_width="223dp"
        android:layout_height="333dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="24dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.445" />
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/GradeList"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        tools:layout_editor_absoluteY="455dp"
        app:layout_constraintVertical_bias="0.521" />

</android.support.constraint.ConstraintLayout>
